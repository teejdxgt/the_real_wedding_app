<header>
  <%= image_tag("logo2.png", :alt => "Sample App", :class => "logo round",) %>
  <nav class="round">
    <ul>
      <li><%= link_to "Home", root_path %></li>
      <% if signed_in? %>
      <li><%= link_to "Music", voting_path %></li>
      <li><%= link_to "Profile", current_user %></li>
      <% end %>
      <li><%= link_to "Details", details_path %></li>
      <% if signed_in? %>
      <li><%= link_to "Sign out", signout_path, :method => :delete %></li>
      <% else %>
      <li><%= link_to "Sign in", signin_path %></li>
      <% end %>
    </ul>
  </nav>
</header>
